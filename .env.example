# 阿里云镜像仓库配置
ACR_REGISTRY=registry.cn-shenzhen.aliyuncs.com
ACR_NAMESPACE=poetize-blog

# MySQL配置
MYSQL_ROOT_PASSWORD=YourSecurePassword123!
MYSQL_DATABASE=poetry
MYSQL_USERNAME=poetry_user
MYSQL_PASSWORD=YourDBPassword123!

# Redis配置
REDIS_PASSWORD=YourRedisPassword123!

# 邮件配置（可选）
MAIL_USERNAME=your_email@example.com
MAIL_PASSWORD=your_smtp_password
MAIL_ENABLE=false

# 域名配置
DOMAIN=yourdomain.com

# JVM配置（优化内存使用）
JVM_XMS=256m
JVM_XMX=512m

# 存储类型
STORE_TYPE=local
LOCAL_UPLOAD_PATH=/app/resourceStorage/
LOCAL_DOWNLOAD_URL=https://ktzxy.top/resourceStorage/

# Spring Boot配置 - 这个很重要，可能就是缺少的变量
SPRING_PROFILES_ACTIVE=prod

# CORS配置（补充）
CORS_ALLOWED_ORIGINS=https://ktzxy.top,https://www.ktzxy.top